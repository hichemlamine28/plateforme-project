{% for host_num in range(71, 77) %}
define host {
    use             linux-server
    host_name       host{{ host_num }}
    alias           host{{ host_num }}.devops6080.com
    address         192.168.56.{{ host_num }}
    check_command   check-host-alive
    max_check_attempts 5
    check_interval  5
    retry_interval  1
    check_period    24x7
    notification_options d,r
    notification_interval 30
    notification_period 24x7
}
{% endfor %}